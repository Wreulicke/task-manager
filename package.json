{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "task-management application",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "watch":"npm-run-all --parallel watch:*",
    "build":"npm-run-all build:js build:bundle",
    "watch:browser":"browser-sync start --server --config bs-config.js",
    "watch:js": "webpack --watch",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:bundle":"vulcanize --inline-scripts index.html --out-html docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wreulicke/task-manager.git"
  },
  "author": "Wreulicke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wreulicke/task-manager/issues"
  },
  "homepage": "https://github.com/Wreulicke/task-manager#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.5",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.5.0",
    "npm-run-all": "^3.1.1",
    "vulcanize": "^1.14.8",
    "webpack": "^1.13.3"
  }
}
